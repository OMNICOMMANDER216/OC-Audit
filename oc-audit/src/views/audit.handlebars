<div class="container">
    {{#if audit}}
<link href="https://{{audit.rootUrl}}/site.css?minify=false" rel="stylesheet">
    <h2 class="center name">{{audit.cuName}}: (<a href="{{audit.url}}" target="_blank"> Go to Site</a> | <a href="/audits/dev/{{audit.id}}">View Code</a>)</h2>
    <br>
    <div class="row">
    <div class="col s12">
      <ul id="tab-nav" class="tabs">
        <li class="tab col s4"><a class="active" href="#diff">Change Highlight</a></li>
        <li class="tab col s4"><a href="#old">Preview Version</a></li>
        <li class="tab col s4 "><a href="#new">New Version</a></li>
      </ul>
    </div>
    <div id="diff" class="tab_content col s12">
        <center>
            {{{audit.diffData}}}
        </center>
    </div>
    <div id="old" class="tab_content col s12">
        <center>
            {{{audit.oldData}}}
        </center>
    </div>
    <div id="new" class="tab_content col s12">
        <center>
            {{{audit.newData}}}
        </center>
    </div>
  </div>
    {{/if}}

</div>